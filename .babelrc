    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "browsers": ["last 2 versions", "safari >= 7", "ie >= 7"]
                },
                "useBuiltIns": "usage",
                "corejs": "3.22"
            }
        ]
    ],
    "plugins": ["@babel/plugin-syntax-dynamic-import"]
}
